	class CuentaCorriente extends Cuenta:
		
		- int cuota;
		- int interes;
		
		- List<Transaccion> transacciones
		
		
		getset de ambos
		
		
		
		class Transaccion extends CuentaCorriente:
		
			enum TipoMov{BIZUM,RECIBOS,TRANSFERENCIAS,IMPUESTOS}
			- double porcentaje;
			
			- int dia;
			- int mes;
			- int ano;
			- String concepto;
			- double importe;
			
			+ getPorcentaje(): double
			
			+ getDia(): int
			
			+ setDia(): void
			
			+ getMes(): int
			
			+ setMes(): void
			
			+ getAno(): int
			
			+ setAno(): void
			
			+ getConcepto(): String
			
			+ setConcepto(): void
			
			+ getImporte(): double
			
			+ setImporte(): void
			
			+ Transaccion()
			
			+ Transaccion(porcentaje,dia,mes,ano,concepto,importe)
		